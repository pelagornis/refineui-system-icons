{
  "name": "refineui-system-icons",
  "version": "0.1.30",
  "description": "RefineUI System Icons - Cross-platform icon system",
  "private": true,
  "type": "commonjs",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces",
    "generate:metadata": "node scripts/generate-metadata.js",
    "generate:web-icons": "python3 scripts/generate_web_icons.py",
    "sync:web-icons-unicode": "node scripts/sync-web-icons-unicode.js",
    "generate:platforms": "python3 scripts/generate_platforms.py",
    "generate:android": "python3 scripts/generate_android_xml.py",
    "generate:ios": "python3 scripts/generate_ios_swift.py",
    "generate:flutter": "python3 scripts/generate_flutter_dart.py",
    "generate:ttf": "fontforge -script scripts/generate_ttf_from_svg.py",
    "generate:fonts": "python3 scripts/generate_font_css.py",
    "build:fonts": "python3 scripts/build_fonts.py",
    "copy:fonts": "python3 scripts/copy_fonts_to_packages.py",
    "build:platforms": "python3 scripts/build_platforms.py",
    "build:all": "python3 scripts/build_all.py",
    "extract:figma": "python3 scripts/figma_icon_extractor.py",
    "release:all": "python3 scripts/release_all_platforms.py",
    "version:bump": "python3 scripts/version_bump.py",
    "clean": "npm run clean --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.5.0",
    "fs-extra": "^11.0.0"
  },
  "keywords": [
    "icons",
    "pelagornis",
    "refineui",
    "system-icons",
    "ui",
    "react",
    "react-native",
    "web",
    "flutter",
    "ios",
    "android"
  ],
  "author": "Pelagornis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pelagornis/refineui-system-icons.git"
  }
}
