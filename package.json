{
  "name": "refineui-system-icons",
  "version": "1.0.0",
  "description": "RefineUI System Icons - Cross-platform icon system",
  "private": true,
  "type": "commonjs",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces",
    "generate:metadata": "node scripts/generate-metadata.js",
    "clean": "npm run clean --workspaces",

    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.5.0"
  },
  "keywords": [
    "icons",
    "refineui",
    "system-icons",
    "ui",
    "react",
    "react-native",
    "web"
  ],
  "author": "RefineUI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/refineui/system-icons.git"
  }
}
