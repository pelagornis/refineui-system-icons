{
  "name": "@refineui/icons-web",
  "version": "1.0.0",
  "description": "RefineUI System Icons Web Package - TypeScript-first web icon library",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "public",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:icons && rollup -c",
    "build:icons": "node scripts/build-icons.js",
    "build:lib": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rimraf dist public/icons",
    "serve": "http-server public -p 3000",
    "prepublishOnly": "npm run clean && npm run build",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "rollup": "^3.0.0",
    "rollup-plugin-typescript2": "^0.34.0",
    "typescript": "^5.3.0",
    "rimraf": "^3.0.2",
    "http-server": "^14.1.1",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.10.0",
    "fs-extra": "^11.1.1"
  },
  "keywords": [
    "refineui",
    "icons",
    "web",
    "typescript",
    "svg",
    "react",
    "vue",
    "angular"
  ],
  "author": "Pelagornis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pelagornis/refineui-system-icons.git",
    "directory": "packages/icons-web"
  },
  "bugs": {
    "url": "https://github.com/pelagornis/refineui-system-icons/issues"
  },
  "homepage": "https://github.com/pelagornis/refineui-system-icons#readme"
}
