{
  "name": "@refineui/web-icons",
  "version": "1.0.0",
  "description": "RefineUI System Icons for Web",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/metadata.json"
  ],
  "scripts": {
    "build": "rollup -c && npm run copy-assets",
    "dev": "rollup -c -w",
    "clean": "rimraf dist",
    "copy-assets": "mkdir -p dist && cp ../../fonts/*.css dist/ && cp ../../fonts/*.woff2 dist/ && cp ../../fonts/*.otf dist/ && cp ../../fonts/*.ttf dist/"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.5.0"
  },
  "keywords": [
    "web",
    "icons",
    "refineui",
    "system-icons",
    "ui",
    "flutter-style"
  ],
  "author": "RefineUI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/refineui/system-icons.git"
  }
}
