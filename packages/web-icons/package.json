{
  "name": "@refineui/web-icons",
  "version": "0.3.22",
  "description": "RefineUI System Icons for Web",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/fonts/refineui-system-icons.css": "./dist/fonts/refineui-system-icons.css",
    "./dist/fonts/refineui-system-icons-regular.css": "./dist/fonts/refineui-system-icons-regular.css",
    "./dist/fonts/refineui-system-icons-filled.css": "./dist/fonts/refineui-system-icons-filled.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src/metadata.json"
  ],
  "scripts": {
    "build": "rollup -c && npm run copy-fonts",
    "copy-fonts": "mkdir -p dist/fonts && cp fonts/*.woff2 dist/fonts/ && (cp fonts/*.woff dist/fonts/ || true) && cp fonts/*.css dist/fonts/",
    "prepublishOnly": "npm run build",
    "dev": "rollup -c -w",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.5.0"
  },
  "keywords": [
    "web",
    "icons",
    "refineui",
    "system-icons",
    "pelagornis",
    "ui"
  ],
  "author": "Pelagornis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pelagornis/refineui-system-icons.git"
  }
}
