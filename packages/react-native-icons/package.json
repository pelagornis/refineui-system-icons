{
  "name": "@refineui/react-native-icons",
  "version": "0.3.15",
  "description": "RefineUI System Icons for React Native",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/metadata.json"
  ],
  "scripts": {
    "build": "rollup -c && npm run copy-fonts",
    "copy-fonts": "mkdir -p dist/fonts && cp fonts/*.woff2 dist/fonts/ && cp fonts/*.css dist/fonts/",
    "dev": "rollup -c -w",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/react": "^18.3.24",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-typescript2": "^0.31.0",
    "typescript": "^4.5.0"
  },
  "keywords": [
    "react-native",
    "icons",
    "refineui",
    "system-icons",
    "pelagornis",
    "ui"
  ],
  "author": "Pelagornis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pelagornis/refineui-system-icons.git"
  }
}
