{
  "name": "@refineui/react-icons",
  "version": "1.0.0",
  "description": "Refine UI System Icons for React",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rimraf dist",
    "generate:icon-constants": "ts-node scripts/generate-icon-constants.ts",
    "prepublishOnly": "npm run clean && npm run generate:icon-constants && npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-typescript2": "^0.34.0",
    "typescript": "^4.9.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.0",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup-plugin-copy": "^3.5.0"
  },
  "keywords": [
    "react",
    "icons",
    "refineui",
    "system-icons",
    "svg"
  ],
  "author": "Pelagornis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pelagornis/refineui-system-icons.git",
    "directory": "packages/react-icons"
  }
}
