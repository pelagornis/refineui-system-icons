{
  "name": "@refineui/icon-cdn",
  "version": "0.1.30",
  "description": "RefineUI System Icons CDN Package",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && node dist/build.js && npm run build:umd && npm run copy-fonts",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "build:umd": "rollup -c",
    "copy-fonts": "mkdir -p dist/fonts && cp fonts/*.woff2 dist/fonts/ && cp fonts/*.css dist/fonts/",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "refineui",
    "icons",
    "cdn",
    "svg",
    "system-icons"
  ],
  "author": "Pelagornis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pelagornis/refineui-system-icons.git"
  },
  "bugs": {
    "url": "https://github.com/pelagornis/refineui-system-icons/issues"
  },
  "homepage": "https://github.com/pelagornis/refineui-system-icons#readme",
  "files": [
    "icons/**/*.svg",
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "fs-extra": "^11.1.1",
    "rollup": "^2.79.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^5.3.0"
  }
}
